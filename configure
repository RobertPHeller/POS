                                { $as_echo "$as_me:$LINENO: WARNING: SWIG version >= 1.3 is required.  You have $swig_version." >&5
$as_echo "$as_me: WARNING: SWIG version >= 1.3 is required.  You have $swig_version." >&2;}
                                SWIG=''

