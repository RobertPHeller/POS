                                { $as_echo "$as_me:$LINENO: result: $SWIG_LIB" >&5
$as_echo "$SWIG_LIB" >&6; }

                        fi
                else
                        { $as_echo "$as_me:$LINENO: WARNING: cannot determine SWIG version" >&5
$as_echo "$as_me: WARNING: cannot determine SWIG version" >&2;}
                        SWIG=''

                fi
        fi




